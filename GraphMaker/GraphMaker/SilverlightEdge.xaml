<UserControl x:Class="GraphMaker.SilverlightEdge"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="50" d:DesignWidth="50" xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk">
    
    <Grid Height="20" Width="20" x:Name="LayoutRoot" Background="Transparent">
        <Ellipse Name="Ellipse" Width="20" Height="20" Fill="White"  StrokeThickness="3">           
        </Ellipse>
        <sdk:Label Height="20" HorizontalAlignment="Left" Margin="5,3,0,0" Name="lblEdgeNumber" VerticalAlignment="Top" Width="21" FontSize="9" FontWeight="Bold" Foreground="DarkBlue" >
            1
        </sdk:Label>
    </Grid>
</UserControl>
