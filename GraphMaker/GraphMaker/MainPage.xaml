        <Button Content="Calculate Dist" Height="23" HorizontalAlignment="Left" Margin="221,477,0,0" Name="btnCalcDist" VerticalAlignment="Top" Width="116" Click="btnCalcDist_Click" />
        <Button Content="Next" Height="23" HorizontalAlignment="Left" Margin="342,477,0,0" Name="btnNext" VerticalAlignment="Top" Width="75" Click="btnNext_Click" />
        <Button Content="Best" Height="23" HorizontalAlignment="Right" Margin="0,477,4,0" Name="btnBest" VerticalAlignment="Top" Width="75" />
